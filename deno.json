{
  "imports": {
    "@oak/oak": "jsr:@oak/oak@^17.2.0",
    "@tajpouria/cors": "jsr:@tajpouria/cors@^1.2.1"
  },
  "tasks": {
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build",
    "server:start": "deno run -A --watch ./src/App.jsx",
    "serve": "deno run build && deno run server:start"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "types": [
      "react",
      "react-dom",
      "@types/react"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
